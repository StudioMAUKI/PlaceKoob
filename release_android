# sh /Users/hoonja/Documents/workspace/app/PlaceKoob/deploy
# cd /Users/hoonja/Documents/workspace/release/PlaceKoob
# sh /Users/hoonja/Documents/workspace/release/PlaceKoob/deploy_remove
cordova build android --release
cp /Users/hoonja/Documents/workspace/release/PlaceKoob/platforms/android/build/outputs/apk/android-release-unsigned.apk /Users/hoonja/Documents/workspace/release/output/
cd /Users/hoonja/Documents/workspace/release/output
jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore mauki_android_key.keystore android-release-unsigned.apk mauki_android
rm PlaceKoob.apk
~/Library/Android/sdk/build-tools/23.0.2/zipalign -v 4 android-release-unsigned.apk PlaceKoob.apk
